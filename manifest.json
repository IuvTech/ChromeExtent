{
  "manifest_version": 3,
  "name": "Stylish Text Generator",
  "version": "1.0",
  "description": "Generate stylish Unicode text for Instagram, TikTok, and more.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "clipboardWrite"
  ],
  "homepage_url": "https://geradornicks.com.br/fontes-para-instagram/"
}